<table class="table table-striped">
  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th></th>
  </thead>
  <tbody>

    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.first_name %></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.email %></td>
        <td><a href="/contacts/<%= contact.id %>" class="btn btn-primary">View</a></td>
      </tr>
    <% end %>

  </tbody>
</table>
